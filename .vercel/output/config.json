{
  "version": 3,
  "routes": [
    {
      "src": "^/(?:(.+)/)?index(?:\\.html)?/?$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "src": "^/(.*)\\.html/?$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "src": "^/(.*)/$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "src": "^(?:/(.*))$",
      "headers": {
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block",
        "X-Content-Type-Options": "nosniff",
        "Referrer-Policy": "strict-origin-when-cross-origin"
      },
      "continue": true
    },
    {
      "src": "^/css(?:/(.*))$",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "^/js(?:/(.*))$",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "^/images(?:/(.*))$",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404"
    }
  ],
  "overrides": {
    ".trunk/plugins/trunk/linters/djlint/test_data/logout.in.html": {
      "path": ".trunk/plugins/trunk/linters/djlint/test_data/logout.in"
    },
    "projects/morrison.html": {
      "path": "projects/morrison"
    },
    "index.html": {
      "path": "index"
    }
  },
  "crons": []
}
